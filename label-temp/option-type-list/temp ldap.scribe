  requestScript = "(&(objectClass=user)(cn=<%=phrase%>*))"
  sourceUrl = "ldap://10.32.21.253:389/DC=test,DC=morpheusdata,DC=com"
  translationScript = <<EOFSCRIBE
for(var x=0;x < data.length ; x++) {
    var row = data[x];
    var a = {};
