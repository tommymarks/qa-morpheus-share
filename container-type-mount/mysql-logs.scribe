resource "container-type-mount" "mysql-logs" {
  code = "mysql-logs"
  name = "mysql logs"
  autoRun = true
  canPersist = false
  certStore = false
  containerPath = "/var/log/mysql"
  customType = false
  deployable = false
  hostPath = "/logs"
  mountType = "volume"
  required = false
  shortName = "logs"
  sortOrder = 3
  subMount = false
  unmanagedEnabled = false
  visible = true
  volumeName = "logs"
}