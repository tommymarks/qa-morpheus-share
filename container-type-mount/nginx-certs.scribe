resource "container-type-mount" "nginx-certs" {
  code = "nginx-certs"
  name = "nginx certs"
  autoRun = true
  canPersist = false
  certStore = true
  containerPath = "/etc/nginx/certs"
  customType = false
  deployable = false
  hostPath = "/data"
  mountType = "volume"
  required = false
  shortName = "certs"
  sortOrder = 2
  subMount = true
  subPath = "certs"
  unmanagedEnabled = false
  visible = true
  volumeName = "data"
}