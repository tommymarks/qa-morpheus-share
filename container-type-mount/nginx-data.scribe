resource "container-type-mount" "nginx-data" {
  code = "nginx-data"
  name = "nginx data"
  autoRun = false
  canPersist = true
  certStore = false
  claimName = "data"
  containerPath = "/var/www/html"
  customType = false
  deployable = true
  hostPath = "/data"
  mountType = "volume"
  required = false
  shortName = "data"
  sortOrder = 1
  subMount = false
  subPath = "html"
  unmanagedEnabled = false
  visible = true
  volumeName = "data"
}