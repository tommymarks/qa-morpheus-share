resource "container-type-mount" "apache.docker.mount.data" {
  code = "apache.docker.mount.data"
  name = "apache data"
  autoRun = true
  canPersist = true
  certStore = false
  containerPath = "/var/www"
  customType = false
  deployable = true
  hostPath = "/data"
  mountType = "volume"
  required = false
  shortName = "data"
  sortOrder = 1
  subMount = false
  unmanagedEnabled = false
  visible = true
}